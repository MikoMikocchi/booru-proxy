{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": [
      "ts-jest",
      {
        "tsconfig": "tsconfig.e2e.json"
      }
    ]
  },
  "collectCoverageFrom": [
    "../src/**/*.(t|j)s"
  ],
  "coverageDirectory": "../coverage",
  "testTimeout": 30000
}